package feathers.data;

/**
	Constants for sorting of items in a collection.

	Note: Some constants may not be valid for certain properties. Please see
	the description of the property in the API reference for complete details.

	@since 1.0.0
**/
enum SortOrder {
	/**
		Indicates that items are sorted in ascending order.

		@since 1.0.0
	**/
	ASCENDING;

	/**
		Indicates that items are sorted in descending order.

		@since 1.0.0
	**/
	DESCENDING;

	/**
		Indicates that items are not sorted.

		@since 1.0.0
	**/
	NONE;
}
